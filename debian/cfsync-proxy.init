description     "cfsync-proxy"

start on filesystem or runlevel [2345]
stop on runlevel [!2345]

setuid swift

kill signal INT

respawn
respawn limit 10 5
umask 022

console log

pre-start script
    test -x /usr/bin/cfsync-proxy || { stop; exit 0; }
end script
[ -f /etc/default/cfsync-proxy ] && . /etc/default/cfsync-proxy
# Start
exec /usr/bin/cfsync-proxy $EOPTS
